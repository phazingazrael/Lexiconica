{
  "name": "lexiconica",
  "version": "0.1.6",
  "private": true,
  "description": "Lexiconica is a 5th edition Dungeons & Dragons campaign management utility developed to be SRD Compliant.",
  "homepage": "http://lexiconica.vercel.app",
  "author": {
    "name": "Phazingazrael",
    "email": "phazedverse@gmail.com",
    "url": "http://phazingazrael.github.io"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "build": "react-app-rewired build",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "eject": "react-app-rewired eject",
    "electron": "wait-on tcp:3000 && electron .",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "start": "react-app-rewired start",
    "start electron": "electron .",
    "start-forge": "electron-forge start",
    "test": "react-app-rewired test",
    "updater": "ncu -u",
    "version-up": "node ./node_modules/node-version-up/index.js"
  },
  "config": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^4.6.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@mui/icons-material": "^5.8.0",
    "@lexical/react": "^0.2.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "dnd-character-sheets": "^1.0.3",
    "lexical": "^0.2.9",
    "markdown-to-jsx": "^7.1.7",
    "nanoid": "^3.3.4",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-social-icons": "^5.13.0",
    "store": "^2.0.12",
    "underscore": "^1.13.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "buffer": "^6.0.3",
    "concurrently": "^7.1.0",
    "node-version-up": "^1.1.0",
    "nth-check": "^2.0.1",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "wait-on": "^6.0.1"
  }
}
