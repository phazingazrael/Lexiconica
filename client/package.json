{
  "name": "lexiconica-client",
  "version": "0.1.5b",
  "private": true,
  "description": "Lexiconica is a 5th edition Dungeons & Dragons campaign management utility developed to be SRD Compliant.",
  "homepage": "http://lexiconica.vercel.app",
  "author": {
    "name": "Phazingazrael",
    "email": "phazedverse@gmail.com",
    "url": "http://phazingazrael.github.io"
  },
  "main": "public/electron.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "build": "react-app-rewired build",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "eject": "react-app-rewired eject",
    "electron": "wait-on tcp:3000 && electron .",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "start": "react-app-rewired start  && server.js",
    "start electron": "electron .",
    "start-forge": "electron-forge start",
    "test": "react-app-rewired test",
    "updater": "ncu -u",
    "version-up": "node ./node_modules/node-version-up/index.js"
  },
  "engines": {
    "node": "16.x"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "lexiconica"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^4.2.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@lexical/react": "^0.2.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "cors": "^2.8.5",
    "dnd-character-sheets": "^1.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "level": "^8.0.0",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "lexical": "^0.2.5",
    "markdown-to-jsx": "^7.1.7",
    "nanoid": "^3.3.3",
    "node-json-db": "^1.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-social-icons": "^5.13.0",
    "subleveldown": "^6.0.1",
    "underscore": "^1.13.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "buffer": "^6.0.3",
    "concurrently": "^7.1.0",
    "electron": "^18.2.0",
    "electron-is-dev": "^2.0.0",
    "node-version-up": "^1.1.0",
    "nth-check": "^2.0.1",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "wait-on": "^6.0.1"
  }
}
